{
  "name": "@ringods/sanity-plugin-github",
  "version": "0.0.5",
  "description": "Sanity.io plugin to include content from Github repositories & gists",
  "author": "Ringo De Smet",
  "license": "MIT",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "watch": "pkg-utils watch --strict",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringods/sanity-plugin-github.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "bugs": {
    "url": "https://github.com/ringods/sanity-plugin-github/issues"
  },
  "homepage": "https://github.com/ringods/sanity-plugin-github#readme",
  "devDependencies": {
    "@sanity/pkg-utils": "^2.2.15",
    "@sanity/plugin-kit": "^3.1.7",
    "@types/node": "^20.2.1",
    "sanity": "^3.11.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "sanity": "^3.11.1"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4"
  }
}
